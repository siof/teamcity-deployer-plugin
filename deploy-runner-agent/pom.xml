        <dependency>
            <groupId>org.codehaus.cargo</groupId>
            <artifactId>cargo-core-uberjar</artifactId>
          <version>1.5.0</version>
            <exclusions>
                <exclusion>
                    <artifactId>xercesImpl</artifactId>
